<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.desafioandroidcore2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Desafio">
        <activity android:name="com.example.desafioandroidcore2.cardapioActivity"></activity>
        <activity android:name="com.example.desafioandroidcore2.homeActivity" />
        <activity android:name="com.example.desafioandroidcore2.splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.desafioandroidcore2.registerActivity" />
        <activity android:name="com.example.desafioandroidcore2.mainActivity" />
        <activity
            android:name="com.example.desafioandroidcore2.loginActivity"
            android:label="Login" />
    </application>

</manifest>